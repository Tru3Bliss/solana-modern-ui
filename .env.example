# for fetching repos and data from GitHub (e.g. for SIMD)
NEXT_PUBLIC_GITHUB_TOKEN=

## Sentry API keys
# for error logging
SENTRY_AUTH_TOKEN=
NEXT_PUBLIC_SENTRY_DSN=

## Algolia API keys
# for full site search
NEXT_PUBLIC_ALGOLIA_APP_ID=
NEXT_PUBLIC_ALGOLIA_API_KEY=
NEXT_PUBLIC_ALGOLIA_SOURCE_ID=

