
## Official SolDev API endpoint
NEXT_PUBLIC_API_ENDPOINT=

## Solana RPC URL
NEXT_PUBLIC_NODE_URL=

## irongforge.cloud data URL
# used to fetch the IDL data for the registry
NEXT_PUBLIC_IRONFORGE_API=

